<% | Hash[String[1], Hash[String[1],
     Variant[String[1],Numeric]]] $config | -%>
#
# Managed by Puppet
#
<% sort(keys($config)).each |$section| { -%>

[<%= $section %>]
<% sort(keys($config[$section])).each |$key| { -%>
<%= $key %>=<% if (' ' in $config[$section][$key]) { -%>"<%= $config[$section][$key] %>"<% } else { -%><%= $config[$section][$key] %><% } %>
<% } -%>
<% } -%>
